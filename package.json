{
  "name": "react-app-01",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sintuba.github.io/react-app01/",
  "engines": {
    "node": "20.9.0",
    "npm": "10.1.0"
  },
  "dependencies": {
    // 依存関係のリスト...
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    // "postbuild" スクリプトは build フォルダ内の index.html を 404.html にコピーするもの
    "postbuild": "cp build/index.html build/404.html",
    // "rm" スクリプトは不要な "docs" フォルダを削除するもの
    "rm": "rm -rf docs",
    // "mv" スクリプトは build フォルダを docs フォルダに移動するもの
    "mv": "mv build docs",
    // "git" スクリプトは自動的にコミットとプッシュを行うもので、慎重に使用してください
    "git": "git add . && git commit && git push origin master",
    // "deploy" スクリプトは "gh-pages" パッケージを使用して "docs" フォルダをデプロイします
    "deploy": "gh-pages -d docs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
